    <!-- Widget Style  -->
    <!-- push/location settings theme. -->
    <style name="PushAndLocationSettingsButton" parent="Widget.AppCompat.Button">
        <item name="android:minHeight">0dp</item>
        <item name="android:minWidth">0dp</item>
    </style>


    <!-- padding 0dp button style -->
    <style name="RemovedPaddingButton" parent="Widget.AppCompat.Button">
        <item name="android:minHeight">0dp</item>
        <item name="android:minWidth">0dp</item>
    </style>

    <!-- padding 0dp button style -->

    <!-- seekbar style -->
    <style name="SeekbarStyle">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@android:drawable/progress_horizontal</item>
        <item name="android:minHeight">0dip</item>
        <item name="android:maxHeight">4dip</item>
        <item name="android:focusable">true</item>
        <item name="android:colorControlHighlight">@android:color/transparent</item>
    </style>

